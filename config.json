{
    "linkedin.com": {
        "company": {
            "selector": ".pv-text-details__right-panel li:nth-of-type(1) button span div",
            "transform": "innerText"
        },
        "container": {
            "selector": ".ph5.pb5"
        },
        "full_name": {
            "selector": "h1.text-heading-xlarge.inline.t-24.v-align-middle.break-words",
            "transform": "innerText"
        },
        "idUser": {
            "selector": "[data-member-id]",
            "transform": "data-member-id"
        },
        "linkedin_graphql_api": {
            "url_template": "https://www.linkedin.com/voyager/api/graphql?includeWebMetadata=true&variables=(memberIdentity:{linkedin_id})&queryId=voyagerIdentityDashProfiles.84cab0be7183be5d0b8e79cd7d5ffb7b"
        },
        "location": {
            "selector": ".mt2.relative div:nth-of-type(2) span",
            "transform": "innerText"
        },
        "pic_url": {
            "selector": "img",
            "transform": "src"
        },
        "titles": {
            "selector": ".text-body-medium.break-words",
            "transform": "innerText"
        },
        "university": {
            "selector": ".pv-text-details__right-panel li:nth-of-type(2) button span div",
            "transform": "innerText"
        }
    },
    "mail.google.com": {
        "dom": {
            "selector": ".hx .gE"
        },
        "email": {
            "selector": ".gD",
            "transform": "email"
        },
        "nameprofile": {
            "selector": ".gD",
            "transform": "innerText"
        }
    },
    "version": "1.0.0"
}
